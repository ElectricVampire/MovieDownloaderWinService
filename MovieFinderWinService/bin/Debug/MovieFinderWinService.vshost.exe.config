<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <appSettings>
    <add key="LogFilePath" value="C:\Users\pareeka2\Desktop\Log.txt"/>
    <!-- Source folder which are going to get monitored should be saperated comma inside value ex. value = "path1,path2"-->
    <add key="SourceFolders" value="C:\Users\pareeka2\Desktop\FPLDownloads"/>
    <!-- Folder where created file from source folder need to be copied -->
    <add key="DestinationFolder" value="C:\Users\pareeka2\Desktop\build"/>
    <!-- Type of created files in which matters for us ex. value=".mp4,.mkv"-->
    <add key="ExtensionFilters" value="*.*"/>
    <!-- File is found in use while trying to copy. We will wait till threadSleepTime(ms) before next copy try of same file -->
    <add key="ThreadSleepTime" value="100"/>
  </appSettings>
</configuration>